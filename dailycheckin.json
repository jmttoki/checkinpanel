            "name": "签到仓库",
            "type": "schedule",
            "time": "0",
            "job": {
                "type": "exec",
                "target": "git clone https://github.com/Oreomeow/checkinpanel -cwd script/Shell"
            },
            "running": true
        },
        {
            "name": "手动更新",
            "type": "schedule",
            "time": "0",
            "job": {
                "type": "exec",
                "target": "git checkout master && git fetch --all && git reset --hard origin/master && git pull -cwd script/Shell/checkinpanel"
            },
            "running": false
        },
        {
            "name": "自动更新",
            "type": "cron",
            "time": "0 16 * * *",
            "job": {
                "type": "exec",
